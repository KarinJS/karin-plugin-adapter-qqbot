    "silk-wasm": "^3.6.3",
    "tweetnacl": "^1.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/form-data": "^2.5.2",
    "@types/node": "^20.17.8",
    "@types/qrcode": "^1.5.5",
    "@types/ws": "^8.5.13",
    "eslint": "^9.7.0",
    "neostandard": "^0.11.9",
    "node-karin": "0.12.25-beta.pr.206.1-1734343074",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
