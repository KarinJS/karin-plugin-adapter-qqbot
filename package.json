{
  "name": "@karinjs/adapter-qqbot",
  "version": "1.2.10",
  "description": "karin plugin adapter for qqbot",
  "homepage": "https://github.com/KarinJS/karin-plugin-adapter-qqbot",
  "bugs": {
    "url": "https://github.com/KarinJS/karin-plugin-adapter-qqbot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarinJS/karin-plugin-adapter-qqbot.git"
  },
  "author": "shijin",
  "type": "module",
  "main": "lib/index.js",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "config",
    "resources",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && tsup",
    "pr": "node lib/cli/pr.js",
    "pub": "npm publish --access public",
    "pub-beta": "npm publish --tag beta",
    "sort": "npx sort-package-json",
    "dev": "tsx src/index.ts",
    "karin": "karin"
  },
  "dependencies": {
    "@karinjs/geturls": "^1.0.2",
    "form-data": "^4.0.2",
    "image-size": "^1.2.0",
    "qrcode": "^1.5.4",
    "silk-wasm": "^3.6.3",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/form-data": "^2.5.2",
    "@types/node": "^20.17.19",
    "@types/qrcode": "^1.5.5",
    "eslint": "^9.21.0",
    "neostandard": "^0.12.1",
    "node-karin": "1.3.15",
    "tsup": "^8.3.6",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "karin": {
    "main": "src/index.ts",
    "apps": [
      "lib/apps"
    ],
    "ts-apps": [
      "src/apps"
    ],
    "static": [
      "resources"
    ],
    "files": [
      "config",
      "data",
      "resources"
    ]
  }
}